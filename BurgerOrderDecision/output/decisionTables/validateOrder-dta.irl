// begin DT validateOrder
// -- begin rule 'validateOrder 0'
package decisionTables {
   rule validateOrder_0 {
      property ilog.rules.business_name = "validateOrder 1";
      property ilog.rules.dt = "decisionTables.validateOrder";
      property ilog.rules.group = "decisionTables.validateOrder";
      property ilog.rules.package_name = "decisionTables";
      property status = "new";
      when {
         com.burger.OrderBurger() from myOrder;
         variable1: com.burger.OrderBurger() from myOrder;
         evaluate (((variable1.rate < 1)));
      } then {
         result = "Invalid Rate for Size";
         ?context.updateContext();
      }
   }
}

// -- end rule 'validateOrder 0'
// -- begin rule 'validateOrder 1'
package decisionTables {
   rule validateOrder_1 {
      property ilog.rules.business_name = "validateOrder 2";
