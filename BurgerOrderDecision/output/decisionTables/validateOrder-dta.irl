      property status = "new";
      when {
         com.burger.OrderBurger() from myOrder;
         variable1: com.burger.OrderBurger() from myOrder;
         evaluate (((variable1.rate in ]1, 100])));
      } then {
         result = "Small Burger";
         ?context.updateContext();
      }
   }
}

